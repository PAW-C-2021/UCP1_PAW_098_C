@model IEnumerable<UPC1_20190140098.Models.PetugasApotek>

@{
    ViewData["Title"] = "Index";
}

<h1>Petugas Apotek</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdObatNavigation)

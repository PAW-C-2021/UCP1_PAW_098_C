            <th>
                @Html.DisplayNameFor(model => model.IdPembeliNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdObatNavigation.IdObat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPembeliNavigation.IdPembeli)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdApoteker">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdApoteker">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdApoteker">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

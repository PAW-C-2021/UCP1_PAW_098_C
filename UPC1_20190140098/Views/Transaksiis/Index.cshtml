@model IEnumerable<UPC1_20190140098.Models.Transaksii>

@{
    ViewData["Title"] = "Index";
}

<h1>Transaksi</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdObat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TglTransaksi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalHarga)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPembeliNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdObat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TglTransaksi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalHarga)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPembeliNavigation.IdPembeli)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTransaksii">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTransaksii">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTransaksii">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
